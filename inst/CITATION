citEntry(entry = "Article",
         author = "Jiada James Zhan, Rebecca A Hodge, Anne L Dunlop, Matthew M Lee, Linh Bui, Donghai Liang, Erin P Ferranti",
         title = "Dietaryindex: A User-Friendly and Versatile R Package for Standardizing Dietary Pattern Analysis in Epidemiological and Clinical Studies",
         journal = "Am J Clin Nutr",
         volume = "",
         number = "",
         pages = "",
         textVersion = paste("Zhan JJ, Hodge RA, Dunlop AL, et al.",
                             "Dietaryindex: a user-friendly and versatile R package for standardizing dietary pattern analysis in epidemiological and clinical studies.",
                             "Am J Clin Nutr", "doi:10.1016/j.ajcnut.2024.08.021"),
         year = "2024",
         publisher = "American Society for Nutrition",
         url = "https://github.com/jamesjiadazhan/dietaryindex",
         header = "A BibTeX entry for LaTeX users is")

citFooter("Our wonderful team thanks you for citing the dietaryindex package.")
